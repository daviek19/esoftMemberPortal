    <link type="text/css" href="@Url.Content(@"~/PortalContent/css/nivo-lightbox-theme/default/default.css")" rel="stylesheet">
    <link type="text/css" href="@Url.Content(@"~/PortalContent/css/animate.css")" rel="stylesheet">

    <!-- #SMARTADMIN LANDING CSS -->
