<body data-spy="scroll">

    <div class="container">

        <!-- Section: #MENU -->
        <ul id="gn-menu" class="gn-menu-main">
            <li class="gn-trigger">
                <a class="gn-icon gn-icon-menu"><span>Menu</span></a>
                <nav class="gn-menu-wrapper">
                    <div class="gn-scroller">
                        <ul class="gn-menu">
                            <li class="gn-search-item">
                                <input placeholder="Search" type="search" class="gn-search">
                                <a class="gn-icon gn-icon-search"><span>Search</span></a>
                            </li>
                            <li>
                                <a href="@Url.Action("index", "Savings", new { Area = "CustomerServices" })" class="gn-icon gn-icon-article">Savings</a>
                            </li>
                            <li>
                                <a href="#" class="gn-icon gn-icon-article">Loans</a>
                            </li>
                            <li>
                                <a href="#" class="gn-icon gn-icon-article">Shares</a>
                            </li>
                            <li>
                                <a href="#" class="gn-icon gn-icon-article">View Guarantors</a>
                            </li>
                            <li>
                                <a href="#updates" class="gn-icon gn-icon-article">Standing Orders</a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </li>

            <li class="hidden-xs">
                <a href="index.html"><img src="@Url.Content(@"~/PortalContent/img/logo-member-portal.png")" alt="logo" style="width: 135px; margin-top: -4px;" /></a>
            </li>


            <li class="pull-right">
                <a href="#"> <span> Logout </span> <i class="fa fa-power-off"></i> </a>
            </li>

        </ul>
        <!-- Section: #MENU -->

    </div>

    @RenderBody()

    <!-- #FOOTER -->
